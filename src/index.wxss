.ui-review-wrap {
    width: 100vw;
    position: relative;
    z-index: 99999;
}
.origin-img {
    width: 100%;
}
.review-wrap {
    width: 100vw;
    height: 300vh;
    text-align: center;
    position: absolute;
    left: 0;
    top: 0;
}
.ui-drag-wrap {
    width: 100%;
    height: 100%;
    position: relative;
    top: 0;
}
.ui-drag {
    width: 100%;
    height: auto;
}
.origin-ui {
    width: 100%;
    height: auto;
    display: block;
}
.drag-box {
    width: 100%;
}
.setting-bar {
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    background-color: rgba(255, 255, 255, 0.6);
}
.setting-bar-wrap {
    width: 100%;
    display: flex;
    padding-top: 4px;
}
.setting-bar-item {
    flex: 1;
    text-align: center;
}
.setting-btn {
    height: 20px;
    line-height: 20px;
    font-size: 12px;
    display: inline-block;
    padding: 0px 10px;
    border-radius: 50px;
    color: #fff;
    background: linear-gradient(135deg,#fa2c19 0,#fa6419 100%);
    vertical-align: middle;
}
.btn-icon {
    width: 16px;
    height: 16px;
    display: inline-block;
    vertical-align: middle;
    position: relative;
    top: -1px;
}
.opacity-bar {
    width: 100%;
    height: 28px;
    display: flex;
    align-items: center;
    margin: 5px auto 15px;
}
.opacity-title {
    font-size: 14px;
    color: #333;
    padding-left: 20px;
}
.slider-wrap {
    flex: 1;
}
.button-bar {
    width: 100%;
    height: 40px;
    align-items: center;
}
.fixed-nav {
    background: linear-gradient(135deg,#fa192c 0,#fa3f19 100%);
    border-radius: 40px 0 0 40px;
    width: 56px;
    height: 34px;
    padding-left: 10px;
    position: fixed;
    right: 0;
    bottom: 100px;
    z-index: 99999;
    font-size: 12px;
    line-height: 34px;
    color: #fff;
}